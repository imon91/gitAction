<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="SanityTestSuite">
    <listeners>
        <listener class-name="listeners.SapUiTestNgListener"></listener>
    </listeners>
    <test name="SanityTests">
        <groups>
            <run>
                <include name="sanity"/>
            </run>
        </groups>
        <classes>
            <class name="SetUpTests"></class>
            <class name="com.shopf.tests.authentication.LoginTests"></class>
            <class name="com.shopf.tests.pagewise.PickupPageTests"></class>
            <class name="com.shopf.tests.pagewise.ReceiveParcelPageTests"></class>
            <class name="com.shopf.tests.pagewise.ReceiveSellerParcelPageTests"></class>
            <class name="com.shopf.tests.pagewise.ReceiveDeliveryAgentsParcelPageTests"></class>
        </classes>

    </test>
</suite>
