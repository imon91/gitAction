<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="SmokeTestSuite">
    <listeners>
        <listener class-name="listeners.RedxWebUiTestNgListener"></listener>
    </listeners>
    <test name="SmokeTests" preserve-order="true" parallel="false">
        <groups>
            <run>
                <include name="smoke"/>
            </run>
        </groups>
        <classes>
            <class name="com.shopf.tests.SetUpTests"></class>
            <class name="com.shopf.tests.Authentication.AuthenticationTests"></class>
            <class name="com.shopf.tests.Authentication.HomePageTests"></class>
            <class name="com.shopf.tests.Authentication.ParcelPageTests"></class>
            <class name="com.shopf.tests.Authentication.PaymentsPageTests"></class>
            <class name="com.shopf.tests.Authentication.CouponsPageTests"></class>
<!--            <class name="com.shopf.tests.Authentication.CreateParcelPageTests"></class>-->
<!--            <class name="com.shopf.tests.Authentication.CreateBulkParcelPageTests"></class>-->
            <class name="com.shopf.tests.Authentication.TrackParcelPageTests"></class>
            <class name="com.shopf.tests.Authentication.MyShopsPageTests"></class>
            <class name="com.shopf.tests.Authentication.PickupLocationPageTests"></class>
            <class name="com.shopf.tests.Authentication.MyPaymentDetailsPageTests"></class>
            <class name="com.shopf.tests.Authentication.NotificationSettingsPageTests"></class>
            <class name="com.shopf.tests.Authentication.CoverageAreaPageTests"></class>
            <class name="com.shopf.tests.Authentication.ChangePasswordPageTests"></class>
            <class name="com.shopf.tests.Authentication.ForgotPasswordPageTests"></class>
            <class name="com.shopf.tests.Authentication.CreditHistoryPageTests"></class>
        </classes>
    </test>
</suite>
