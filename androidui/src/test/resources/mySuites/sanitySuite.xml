<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="SanityTestSuite">
    <test name="SanityTests" preserve-order="true" parallel="false">
        <parameter name="suite" value="sanity"/>
        <groups>
            <run>
                <include name="sanity"/>
            </run>
        </groups>
        <classes>
            <class name="com.shopf.tests.Authentication"></class>
            <class name="com.shopf.tests.MyBag"></class>
        </classes>
    </test>
</suite>
