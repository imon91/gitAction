<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="RegressionTestSuite">
    <listeners>
        <listener class-name="listerners.AndroidTestNgListener"></listener>
    </listeners>
    <test name="RegressionTests">
        <parameter name="suite" value="regression"/>
            <groups>
                <run>
                    <include name="regression"/>
                </run>
            </groups>
            <classes>
                <class name="com.shopf.tests.Authentication"></class>
                <class name="com.shopf.tests.MyBag"></class>
            </classes>
        </test>
</suite>
